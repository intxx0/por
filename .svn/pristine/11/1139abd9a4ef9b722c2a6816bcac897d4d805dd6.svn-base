<style type="text/css">
    #wizard .label { line-height:18px; }
</style>
<div>
    <!-- begin breadcrumb -->
    <ol class="breadcrumb pull-right">
        <li><a href="javascript:;">{{ 'HOME' | translate }}</a></li>
        <li class="active">{{ 'CONFIGURATION' | translate }}</li>
    </ol>
    <!-- end breadcrumb -->
    <!-- begin page-header -->
    <h1 class="page-header">{{ 'CONFIGURATION' | translate }}</h1>
    <!-- end page-header -->
    <!-- begin row -->
    <div class="row">
        <!-- begin col-12 -->
        <div class="col-md-12 ui-sortable">
            <!-- begin panel -->
            <div class="panel panel-inverse">
                <div class="panel-heading">
                    <h4 class="panel-title">{{ 'CONFIGURATION' | translate }}</h4>
                </div>
                <div class="panel-body">
                    <form action="/" method="POST">
                        <div id="wizard" class="bwizard clearfix">
                            <ol class="bwizard-steps clearfix clickable" role="tablist" style="padding-left: 0px;">
                                <li role="tab" style="z-index: 4; float:left!important;" aria-selected="true" class="active" ng-class="tabsOperacao.tabPesquisar.tabAtivar" ng-show="tabsOperacao.tabPesquisar.tabhabilitar">
                                    <span class="label"><i class="fa fa-search"></i></span>
                                    <a href="javascript:;" class="hidden-phone" ng-click="habilitaDesabilitaAbaPesquisa()" >
                                        {{ 'SEARCH' | translate }}
                                    </a><a href="javascript:;" class="hidden-phone" ng-click="habilitaDesabilitaAbaPesquisa()" ><small>Pesquisar operações.</small></a><a href="#step1" class="hidden-phone">
                                    </a>
                                </li>
                                <li role="tab" style="z-index: 3; float:left!important;" aria-selected="false" class="" ng-class="tabsOperacao.tabCadOperacao.tabAtivar" ng-show="tabsOperacao.tabCadOperacao.tabhabilitar">
                                    <span class="label">1</span><a href="javascript:;" class="hidden-phone" ng-click="habilitaDesabilitaAbaPesquisa()" >
                                        {{ 'OPERATION' | translate }}
                                    </a><a href="#step2" class="hidden-phone"><small>Dados da Operação</small></a><a href="#step2" class="hidden-phone">
                                    </a>
                                </li>
                                <li role="tab" style="z-index: 2; float:left!important;" aria-selected="false" ng-class="tabsOperacao.tabCadFiscalizacao.tabAtivar" ng-show="tabsOperacao.tabCadFiscalizacao.tabhabilitar">
                                    <span class="label">2</span><a href="#step3" class="hidden-phone" ng-click="habilitaDesabilitaAbaCadFiscalizacoesOperacao()" target="_self">
                                        {{ 'SOURCE' | translate }}
                                    </a><a href="#step3" class="hidden-phone"><small>Tipos de Fiscalização</small></a><a href="#step3" class="hidden-phone">
                                    </a>
                                </li>
                                <li role="tab" style="z-index: 1; float:left!important;" aria-selected="false" ng-class="tabsOperacao.tabCadKor.tabAtivar" ng-show="tabsOperacao.tabCadKor.tabhabilitar">
                                    <span class="label">3</span><a href="#step4" class="hidden-phone" ng-click="habilitaDesabilitaAbaCadKor()" >
                                        KOR
                                    </a><a href="#step4" class="hidden-phone" ng-click="habilitaDesabilitaAbaCadKor()"><small>Equipamentos da operação</small></a><a href="#step4" class="hidden-phone">
                                    </a>
                                </li>
                                <li role="tab" style="z-index: 1; float:left!important;" aria-selected="false" ng-class="tabsOperacao.tabCadUsuario.tabAtivar" ng-show="tabsOperacao.tabCadUsuario.tabhabilitar">
                                    <span class="label">4</span><a href="#step5" ng-click="habilitaDesabilitaAbaCadUsuarios()" class="hidden-phone">
                                        {{ 'USERS' | translate }}
                                    </a><a href="#step4" ng-click="habilitaDesabilitaAbaCadUsuarios()" class="hidden-phone"><small>Usuários participantes</small></a><a href="#step4" class="hidden-phone">
                                    </a>
                                </li>
                                <li role="tab" style="z-index: 1; float:left!important;" aria-selected="false" ng-class="tabsOperacao.tabConfirmacaoOperacao.tabAtivar" ng-show="tabsOperacao.tabConfirmacaoOperacao.tabhabilitar">
                                    <span class="label">5</span><a href="#step6" class="hidden-phone">
                                        {{ 'CONFIRM' | translate }}
                                    </a><a href="#step4" class="hidden-phone"><small>Dados da operação</small></a><a href="#step4" class="hidden-phone">
                                    </a>
                                </li>
                                <!--<li role="tab" style="z-index: 1; float:left!important;" aria-selected="false" ng-class="tabsOperacao.tabOperacaoFinalizada.tabAtivar" ng-show="tabsOperacao.tabOperacaoFinalizada.tabhabilitar">
                                    <span class="label">6</span><a href="#step7" class="hidden-phone">
                                        {{ 'FINISHED' | translate }}
                                    </a><a href="#step4" class="hidden-phone"><small>Finalização de configuração</small></a><a href="#step4" class="hidden-phone">
                                    </a>
                                </li>-->
                            </ol>
                            <div class="well">
                                <div ng-class="tabsOperacao.tabPesquisar.contentAtivar" ng-show="tabsOperacao.tabPesquisar.contentHabilitar">
                                    <div id="step1" role="tabpanel" class="bwizard-activated" aria-hidden="false" style="">
                                        <div class="col-md-9" style="margin-bottom:10px;">
                                            <button type="button" class="btn btn-sm btn-success" ng-click="habilitaDesabilitaAbaCadOperacao()"><i class="fa fa-plus" style="font-size:10px;"></i>&nbsp;&nbsp;{{ 'NEW_OPERATION' | translate }}</button>
                                        </div>
                                        <div class="panel panel-inverse" data-sortable-id="table-basic-4">
                                            <div class="panel-body">
                                                <table class="table table-striped table-bordered">
                                                    <thead>
                                                        <tr>
                                                            <th>ID</th>
                                                            <th>{{ 'OPERATION' | translate }}</th>
                                                            <th width="150">{{ 'RESPONSIBLE' | translate }}</th>
                                                            <th width="150">{{ 'OFFICE' | translate }}</th>
                                                            <th width="150">{{ 'INSTITUTION' | translate }}</th>
                                                            <th width="150">{{ 'TELEPHONE' | translate }}</th>
                                                            <th width="150">{{ 'START' | translate }}</th>
                                                            <th width="150">{{ 'END' | translate }}</th>
                                                            <th width="100">Status</th>
                                                            <th width="76">{{ 'ACTION' | translate }}</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="operacao in novaFiscalizacoesOnLine | filter:operacao:restrict">
                                                            <td>{{operacao.IdFiscalizacao}}</td>
                                                            <td>{{operacao.DescOperacao}}</td>
                                                            <td>{{operacao.NomeResponsavel}}</td>
                                                            <td>{{operacao.CargoResponsavel}}</td>
                                                            <td>{{operacao.InstituicaoResponsavel}}</td>
                                                            <td>{{operacao.TelResponsavel}}</td>
                                                            <td>{{operacao.DataCriado | date:'dd/MM/yyyy HH:mm:ss'}}</td>
                                                            <td>{{operacao.DataFinalOperacao | date:'dd/MM/yyyy HH:mm:ss'}}</td>
                                                            <td>{{operacao.Ativo == 1?'Ativado':'Desativado'}}</td>
                                                            <td>
                                                                <a href="javascript:;" ng-click="alterarFiscalizacao(operacao)" class="btn btn-default" style="padding: 2px" tooltip-enable="true" tooltip-trigger="mouseenter" tooltip-placement="bottom" tooltip="Editar">
                                                                    <i class="fa fa-2x fa-pencil-square-o" style="color: green; font-size: 20px;"></i>
                                                                </a>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div id="taboperacoes" ng-class="tabsOperacao.tabCadOperacao.contentAtivar" ng-show="tabsOperacao.tabCadOperacao.contentHabilitar">
                                    <div id="step2" role="tabpanel" class="" aria-hidden="true" style="">
                                        <form name="formCadOperacao" role="form">

                                            <div class="row">
                                                <div class="col-md-6">
                                                    <label class="control-label">{{ 'OPERATION_NAME' | translate }}:</label>
                                                    <input type="text" name="txtNomeOperacao" id="txtNomeOperacao" class="form-control" data-toggle="tooltip" data-trigger="hover" ng-model="novaOperacao.DescOperacao" placeholder="{{ 'ENTER_OPERATION_NAME' | translate }}" required>
                                                    <div ng-show="formCadOperacao.txtNomeOperacao.$invalid && !formCadOperacao.txtNomeOperacao.$pristine" style="color: red;">{{ 'ENTER_OPERATION_NAME' | translate }}</div>
                                                </div>

                                                <div class="col-md-6">
                                                    <label class="">{{ 'NATURE' | translate }}:</label>
                                                    <select class="form-control" id="ddlNaturezaOperacao" name="ddlNaturezaOperacao" ng-model="natureza" ng-options="option.DescnaturezaOperacao for option in NaturezaOperacao track by option.IdNaturezaOperacao" required>
                                                        <option value="">{{ 'SELECT' | translate }}...</option>
                                                    </select>

                                                    <div ng-show="formCadOperacao.ddlNaturezaOperacao.$invalid && !formCadOperacao.ddlNaturezaOperacao.$pristine" style="color: red;">{{ 'SELECT_NATURE' | translate }}</div>

                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <label class="">{{ 'RESPONSIBLE' | translate }}:</label>
                                                    <input type="text" class="form-control" name="txtResponsavelOperacao" id="txtResponsavelOperacao" ng-model="novaOperacao.NomeResponsavel" placeholder="{{ 'OPERATION_RESPONSIBLE' | translate }}" required>
                                                    <div ng-show="formCadOperacao.txtResponsavelOperacao.$invalid && !formCadOperacao.txtResponsavelOperacao.$pristine" style="color: red;">{{ 'ENTER_RESPONSIBLE' | translate }}</div>
                                                </div>
                                                <div class="col-md-4">
                                                    <label class="">{{ 'OFFICE' | translate }}:</label>
                                                    <input type="text" class="form-control" name="txtCargoResponsavel" id="txtCargoResponsavel" ng-model="novaOperacao.CargoResponsavel" placeholder="{{ 'OFFICE_OR_RESPONSIBLE' | translate }}" required>
                                                    <div ng-show="formCadOperacao.txtCargoResponsavel.$invalid && !formCadOperacao.txtCargoResponsavel.$pristine" style="color: red;">{{ 'ENTER_OFFICE' | translate }}</div>
                                                </div>
                                                <div class="col-md-4">
                                                    <label class="">{{ 'INSTITUTION' | translate }}:</label>
                                                    <input type="text" class="form-control" id="txtInstituicaoResponsavel" name="txtInstituicaoResponsavel" ng-model="novaOperacao.InstituicaoResponsavel" placeholder="{{ 'RESPONSIBLE_INSTITUTION' | translate }}" required>
                                                    <div ng-show="formCadOperacao.txtInstituicaoResponsavel.$invalid && !formCadOperacao.txtInstituicaoResponsavel.$pristine" style="color: red;">{{ 'ENTER_INSTITUTION' | translate }}</div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <label class="">E-Mail:</label>
                                                    <input type="email" class="form-control" id="txtEmailResponsavel" name="txtEmailResponsavel" ng-model="novaOperacao.EmailResposnsavel" placeholder="{{ 'RESPONSIBLE_EMAIL' | translate }}" required>
                                                    <div ng-show="formCadOperacao.txtEmailResponsavel.$invalid && !formCadOperacao.txtEmailResponsavel.$pristine" style="color: red;">{{ 'INVALID_EMAIL' | translate }}</div>
                                                </div>
                                                <div class="col-lg-4">
                                                    <label class="">{{ 'MOBILE' | translate }}:</label>
                                                    <input type="tel" class="form-control" id="txtTelefoneResponsavel" name="txtTelefoneResponsavel" ng-model="novaOperacao.TelResponsavel" ui-mask="(99)99999-9999" ui-mask-placeholder ui-mask-placeholder-char="_" required>
                                                    <div ng-show="formCadOperacao.txtTelefoneResponsavel.$invalid && !formCadOperacao.txtTelefoneResponsavel.$pristine" style="color: red;">{{ 'ENTER_TELEPHONE' | translate }}</div>
                                                </div>

                                                <div class="col-md-2">
                                                    <label class="">{{ 'START_DATE' | translate }}:</label>
                                                    <input type="text" class="form-control" name="txtDataCriado" id="txtDataCriado" ng-model="novaOperacao.DataCriado" ui-mask="99/99/9999 as 99:99" ui-mask-placeholder ui-mask-placeholder-char="_" required>
                                                    <div ng-show="formCadOperacao.txtDataCriado.$invalid && !formCadOperacao.txtDataCriado.$pristine" style="color: red;">{{ 'ENTER_START_DATE' | translate }}</div>
                                                </div>
                                                <div class="col-md-2">
                                                    <label class="">{{ 'END_DATE' | translate }}:</label>
                                                    <input type="text" class="form-control" name="txtDataFim" ui-mask="99/99/9999 as 99:99" ui-mask-placeholder ui-mask-placeholder-char="_" ng-model="novaOperacao.DataFinalOperacao" required>
                                                    <div ng-show="formCadOperacao.txtDataInicio.$invalid && !formCadOperacao.txtDataInicio.$pristine" style="color: red;">{{ 'ENTER_END_DATE' | translate }}</div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <label class="">{{ 'OPERATION_DESCRIPTION' | translate }}:</label>
                                                    <textarea id="txtDescOperacao" placeholder="{{ 'OPERATION_DESCRIPTION' | translate }}" ng-model="novaOperacao.DescObservacao" rows="8" class="form-control" required></textarea>
                                                </div>

                                                <div class="col-md-4">
                                                    <label class="">Status:</label>
                                                    <select class="form-control" name="ddlUsuarioAtivo" id="ddlUsuarioAtivo" ng-model="novaOperacao.Ativo" required>
                                                        <option value="">{{ 'SELECT' | translate }}...</option>
                                                        <option value="0">{{ 'ACTIVE' | translate }}</option>
                                                        <option value="1">{{ 'WAITING' | translate }}</option>
                                                        <option value="2">{{ 'INACTIVE' | translate }}</option>
                                                    </select>
                                                    <div ng-show="formCadOperacao.ddlUsuarioAtivo.$invalid && !formCadOperacao.ddlUsuarioAtivo.$pristine" style="color: red;">{{ 'ENTER_END_DATE' | translate }}</div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <button type="button" class="btn btn-sm btn-success" ng-click="habilitaDesabilitaAbaCadFiscalizacoesOperacao()" ng-disabled="formCadOperacao.$invalid" style="float:right;">{{ 'NEXT' | translate }}&nbsp;&nbsp;<i class="fa fa-arrow-right" style="font-size:10px;"></i></button>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                                <div id="tabtipofiscalizacao" ng-class="tabsOperacao.tabCadFiscalizacao.contentAtivar" ng-show="tabsOperacao.tabCadFiscalizacao.contentHabilitar">
                                    <div id="step3" role="tabpanel" class="" aria-hidden="true">
                                        <form name="formTipoFiscalizacao" id="formTipoFiscalizacao" role="form" ng-submit="">
                                            <div class="row text-center" style="margin-bottom: 30px;" ng-show="novaOperacao.DescOperacao">
                                                <div class="col-md-12">
                                                    <label class="label" style="font-size: 20px; color:#242a30;">
                                                        {{novaOperacao.DescOperacao}}
                                                    </label>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-10">
                                                    <label class="control-label">{{ 'SELECT_INSPECT_TYPE' | translate }}:</label>
                                                    <br />
                                                    <br />
                                                    <fieldset style="border: 1px solid #CCD0D4; border-radius: 5px; padding-left:15px; margin-bottom:20px;">
                                                        <div class="row">
                                                            <div class="col-md-12">
                                                                <div class="checkbox">
                                                                    <label style="font-size: 15px;">
                                                                        <input type="checkbox" ng-checked="isChecked()" ng-click="toggleAll()" style="height: 15px;">
                                                                        {{ 'SELECT' | translate }} ..
                                                                    </label>
                                                                </div>
                                                                <div class="checkbox" ng-repeat="item in TipoFiscalizacao" style="margin-left: 20px;">
                                                                    <label style="font-size: 15px;">
                                                                        <input type="checkbox" ng-checked="exists(item, selected)" ng-click="toggle(item, selected)" ng-model="item.status" style="height: 15px;">
                                                                        {{item.DescTipoFiscalizacao}}
                                                                    </label>

                                                                </div>
                                                                <!--<code style="display: block; padding: 8px;">{{selected}}</code>-->

                                                            </div>
                                                        </div>
                                                    </fieldset>
                                                </div>
                                            </div>
                                            <!--<div class="row">
                                                <div class="col-md-6">
                                                    <a data-toggle="tab" href="" ng-click="habilitaDesabilitaAbaCadKor()" target="_self" class="btn btn-success pull-right" ng-show="botaoAvancarTipoFiscalizacao">{{ 'NEXT' | translate }}</a>
                                                    <a data-toggle="tab" href="" ng-click="habilitaDesabilitaAbaCadOperacao()" target="_self" class="btn btn-danger pull-right" style="margin-right: 12px">{{ 'BACK' | translate }}</a>
                                                </div>
                                            </div>-->
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <button type="button" class="btn btn-sm btn-success" ng-click="habilitaDesabilitaAbaCadKor()" ng-disabled="formCadOperacao.$invalid" style="float:right;">{{ 'NEXT' | translate }}&nbsp;&nbsp;<i class="fa fa-arrow-right" style="font-size:10px;"></i></button>
                                                    <button type="button" class="btn btn-sm btn-success" ng-click="habilitaDesabilitaAbaCadOperacao()" ng-disabled="formCadOperacao.$invalid" style="float:left;"><i class="fa fa-arrow-left" style="font-size:10px;"></i>&nbsp;&nbsp;{{ 'BACK' | translate }}</button>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                                <div id="tabkor" ng-class="tabsOperacao.tabCadKor.contentAtivar" ng-show="tabsOperacao.tabCadKor.contentHabilitar">
                                    <div id="step4" role="tabpanel" class="" aria-hidden="true">
                                        <form name="formCadKor" role="form" ng-submit="verificaIdFiscalizacaoOnLine(novaFiscalizacoesOnLine.IdFiscalizacao, natureza)">

                                            <div class="row text-center" style="margin-bottom: 30px;" ng-show="novaOperacao.DescOperacao">
                                                <div class="col-md-12">
                                                    <label class="label" style="font-size: 20px; color:#242a30;">
                                                        {{novaOperacao.DescOperacao}}
                                                    </label>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <label class="control-label">{{ 'SELECT_PART_KOR' | translate }}:</label>
                                                    <br />
                                                    <br />
                                                    <fieldset style="border: 1px solid #CCD0D4; border-radius: 5px; padding-left:15px; margin-bottom:20px;">
                                                        <div class="row">
                                                            <div class="col-md-12">
                                                                <div class="checkbox">
                                                                    <label style="font-size: 15px;">
                                                                        <input type="checkbox" ng-checked="isCheckedkor()" ng-click="toggleAllkor()" style="height: 15px;">
                                                                        {{ 'SELECT' | translate }} ..
                                                                    </label>
                                                                </div>
                                                                <div class="checkbox" ng-repeat="kor in Kors" style="margin-left: 20px;">
                                                                    <label style="font-size: 15px;">
                                                                        <input type="checkbox" ng-checked="existskor(kor, selectedkor)" ng-click="togglekor(kor, selectedkor)" ng-model="kor.status" style="height: 15px;">
                                                                        {{kor.Nome}}
                                                                    </label>
                                                                </div>
                                                                <!--<code style="display: block; padding: 8px;">{{selectedkor}}</code>-->
                                                            </div>
                                                        </div>
                                                    </fieldset>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <button type="button" class="btn btn-sm btn-success" ng-click="habilitaDesabilitaAbaCadUsuarios()" ng-disabled="formCadOperacao.$invalid" style="float:right;">{{ 'NEXT' | translate }}&nbsp;&nbsp;<i class="fa fa-arrow-right" style="font-size:10px;"></i></button>
                                                    <button type="button" class="btn btn-sm btn-success" ng-click="habilitaDesabilitaAbaCadFiscalizacoesOperacao()" ng-disabled="formCadOperacao.$invalid" style="float:left;"><i class="fa fa-arrow-left" style="font-size:10px;"></i>&nbsp;&nbsp;{{ 'BACK' | translate }}</button>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                                <div id="tabusuarios" ng-class="tabsOperacao.tabCadUsuario.contentAtivar" ng-show="tabsOperacao.tabCadUsuario.contentHabilitar">
                                    <div id="step4" role="tabpanel" class="" aria-hidden="true">
                                        <form name="formCadUsuario" role="form">
                                            <div class="row text-center" style="margin-bottom: 30px;" ng-show="novaOperacao.DescOperacao">
                                                <div class="col-md-12">
                                                    <label class="label" style="font-size: 20px; color:#242a30;">
                                                        {{novaOperacao.DescOperacao}}
                                                    </label>
                                                </div>
                                            </div>
                                            <fieldset>
                                                <div class="row">
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label>{{ 'NAME' | translate }}:</label>
                                                            <input type="text" ng-model="novoUsuario.DescNomeUsuario" placeholder="{{ 'ENTER_NAME' | translate }}" id="txtNomeUsuario" class="form-control" required>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label>{{ 'USER' | translate }}:</label>
                                                            <input type="text" ng-model="novoUsuario.DescLoginUsuario" placeholder="{{ 'ENTER_USER' | translate }}" id="txtUsuario" class="form-control" required>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label>{{ 'GROUP' | translate }}:</label>
                                                            <select class="form-control" ng-model="novoUsuario.IdGrupo" id="ddlGrupo" ng-options="option.IdGrupo as option.DescGrupo for option in GrupoUsuarios" required>
                                                                <option value="">{{ 'SELECT' | translate }}...</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label>{{ 'EMAIL' | translate }}:</label>
                                                            <input type="text" ng-model="novoUsuario.DescEmailUsuario" placeholder="{{ 'ENTER_EMAIL' | translate }}" id="txtEmail" class="form-control" required>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label>{{ 'SMTP_SERVER' | translate }}:</label>
                                                            <select class="form-control" id="ddlSmtp" ng-model="smtpusuario" ng-options="option.DescSmtp for option in SmtpEmail track by option.IdSmtp">
                                                                <option value="">{{ 'SELECT' | translate }}...</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label>{{ 'EMAIL_PASSWORD' | translate }}:</label>
                                                            <input type="password" ng-model="novoUsuario.EmailSenha" placeholder="{{ 'ENTER_EMAIL_PASSWORD' | translate }}" id="txtSenhaEmail" class="form-control">
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label>{{ 'KOR_PASSWORD' | translate }}:</label>
                                                            <input type="password" ng-model="novoUsuario.Senha" placeholder="{{ 'ENTER_POR_PASSWORD' | translate }}" id="txtSenha" class="form-control" required>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label>{{ 'CONFIRM_KOR_PASSWORD' | translate }}:</label>
