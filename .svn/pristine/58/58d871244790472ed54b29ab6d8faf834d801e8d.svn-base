<div ng-controller="emailCtrl">
    <!-- begin breadcrumb -->
    <ol class="breadcrumb pull-right">
        <li><a href="javascript:;" ng-href="#/index">{{ 'HOME' | translate }}</a></li>
        <li><a href="javascript:;" ng-href="#/emailsrecebidos">{{ 'EMAILS' | translate }}</a></li>
        <li class="active">{{ 'INBOX' | translate }}</li>
    </ol>
    <!-- end breadcrumb -->
    <!-- begin page-header -->
    <h1 class="page-header">{{ 'EMAILS' | translate }} - {{ 'INBOX' | translate }}</h1>
    <!-- end page-header -->
    <!-- begin row -->

        <!-- begin row -->
        <div class="row">
            <!-- begin col-2 -->
            <div class="col-md-2">
                <form>
                    <div class="input-group m-b-15">
                        <input type="text" class="form-control input-sm input-white" placeholder="{{ 'SEARCH_EMAIL' | translate }}" />
                        <span class="input-group-btn">
                            <button class="btn btn-sm btn-inverse" type="button"><i class="fa fa-search"></i></button>
                        </span>
                    </div>
                </form>
                <div class="hidden-sm hidden-xs">
                    <ul class="nav nav-pills nav-stacked nav-inbox" style="width:180px;">
                        <li class="active">
                            <a ng-href="#/emailsrecebidos">
                                <i class="fa fa-inbox fa-fw m-r-5"></i> {{ 'INBOX' | translate }} ({{EmailsRecebidosTotal}})
                            </a>
                        </li>
                        <li><a ng-href="#/emailsenviados"><i class="fa fa-inbox fa-fw m-r-5"></i> {{ 'SENT' | translate }}</a></li>
                        <li><a ng-href="#/emailsexcluidos"><i class="fa fa-trash-o fa-fw m-r-5"></i> {{ 'DELETED' | translate }}</a></li>
                    </ul>
                    <!--<h5 class="m-t-20">Folders</h5>
                    <ul class="nav nav-pills nav-stacked nav-inbox">
                        <li><a href="#"><i class="fa fa-folder fa-fw m-r-5"></i> Newsletter</a></li>
                        <li><a href="#"><i class="fa fa-folder fa-fw m-r-5"></i> Friend</a></li>
                        <li><a href="#"><i class="fa fa-folder fa-fw m-r-5"></i> Company</a></li>
                        <li><a href="#"><i class="fa fa-folder fa-fw m-r-5"></i> Downloaded</a></li>
                    </ul>-->
                </div>
            </div>
            <!-- end col-2 -->
            <!-- begin col-10 -->
            <div class="col-md-10">
                <div class="email-btn-row hidden-xs">
                    <a  ng-href="#/email" class="btn btn-sm btn-inverse"><i class="fa fa-plus m-r-5"></i> {{ 'NEW_EMAIL' | translate }}</a>
                    <!--<a href="#" class="btn btn-sm btn-default disabled">Reply</a>
                    <a href="#" class="btn btn-sm btn- disabled">Delete</a>
                    <a href="#" class="btn btn-sm btn-default disabled">Archive</a>
                    <a href="#" class="btn btn-sm btn-default disabled">Junk</a>
                    <a href="#" class="btn btn-sm btn-default disabled">Swwep</a>
                    <a href="#" class="btn btn-sm btn-default disabled">Move to</a>
                    <a href="#" class="btn btn-sm btn-default disabled">Categories</a>-->
                </div>
                <div class="email-content">
                    <table class="table table-email">
                        <thead>
                            <tr>
                                <th class="email-select"><a href="#" data-click="email-select-all"><i class="fa fa-square-o fa-fw"></i></a></th>
                                <th colspan="2">
                                    <div class="dropdown">
                                        <a href="#" class="email-header-link" data-toggle="dropdown">{{ 'VIEW' | translate }} <i class="fa fa-angle-down m-l-5"></i></a>
                                        <ul class="dropdown-menu">
                                            <li class="active"><a href="#">{{ 'ALL' | translate }}</a></li>
                                            <li><a href="#">{{ 'UNREAD' | translate }}</a></li>
                                            <li><a href="#">{{ 'READ' | translate }}</a></li>
                                        </ul>
                                    </div>
                                </th>
                                <th>
                                    <div class="dropdown">
                                        <a href="#" class="email-header-link" data-toggle="dropdown">{{ 'ORDER_BY' | translate }} <i class="fa fa-angle-down m-l-5"></i></a>
                                        <ul class="dropdown-menu">
                                            <li class="active"><a href="#">{{ 'DATE' | translate }}</a></li>
                                            <li><a href="#">{{ 'FROM' | translate }}</a></li>
                                            <li><a href="#">{{ 'SUBJECT' | translate }}</a></li>
                                        </ul>
                                    </div>
                                </th>
                            </tr>
                        </thead>
                        <tbody>

                            <tr ng-repeat="recebidos in EmailsRecebidos | orderBy:propertyName:reverse | filter: recebidos:restrict" ng-class="{'emailnaovisualizado':recebidos.Visualizado == false}">
                                <td class="email-select"><a href="#" data-click="email-select-single"><i class="fa fa-square-o fa-fw"></i></a></td>
                                <td class="email-sender">
                                    {{recebidos.Remetente}}
                                </td>
                                <td class="email-subject">
                                    <a ng-click="abreModal(recebidos)" class="email-btn" data-click="email-archive"><i class="fa fa-folder-open"></i></a>
                                    <a ng-click="atualizaCampoDelete(recebidos)" class="email-btn" data-click="email-remove"><i class="fa fa-trash-o"></i></a>
                                    <!--<a href="#" class="email-btn" data-click="email-highlight"><i class="fa fa-flag"></i></a>-->
                                    {{recebidos.Assunto}}
                                </td>
                                <td class="email-date">{{recebidos.DataCriado | date: 'dd/MM/yyyy HH:mm:ss'}}</td>
                            </tr>

                        </tbody>
                    </table>
                    <div class="email-footer clearfix">
                        {{EmailsRecebidosTotal}} {{ 'MESSAGES_TOTAL' | translate }}
                        <custom-pager page="{{page}}" pages-count="{{pagesCount}}" total-count="{{totalCount}}" search-func="emailsRecebidos(page)"></custom-pager>
                    </div>
                </div>
            </div>
            <!-- end col-10 -->
        </div>
        <!-- end row -->
</div>






