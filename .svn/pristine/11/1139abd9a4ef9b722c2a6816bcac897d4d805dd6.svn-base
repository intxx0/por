                                                        <button type="button" class="btn btn-sm btn-success" ng-click="verificaIdUsuarioExiste(novoUsuario)" ng-disabled="formCadUsuario.$invalid"><i class="fa fa-save"></i>&nbsp;&nbsp;{{ 'SAVE' | translate }}</button>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <button type="button" class="btn btn-sm btn-success" ng-click="habilitaDesabilitaAbaConfirmacao()" ng-disabled="formCadOperacao.$invalid" style="float:right;">{{ 'NEXT' | translate }}&nbsp;&nbsp;<i class="fa fa-arrow-right" style="font-size:10px;"></i></button>
                                                        <button type="button" class="btn btn-sm btn-success" ng-click="habilitaDesabilitaAbaCadKor()" ng-disabled="formCadOperacao.$invalid" style="float:left;"><i class="fa fa-arrow-left" style="font-size:10px;"></i>&nbsp;&nbsp;{{ 'BACK' | translate }}</button>
                                                    </div>
                                                </div>
                                            </fieldset>
                                        </form>
                                    </div>
                                </div>
