<div>
    <!-- begin breadcrumb -->
    <ol class="breadcrumb pull-right">
        <li><a href="javascript:;">{{ 'HOME' | translate }}</a></li>
        <li class="active">{{ 'REGISTER_KOR' | translate }}</li>
    </ol>
    <!-- end breadcrumb -->
    <!-- begin page-header -->
    <h1 class="page-header">{{ 'REGISTER_KOR' | translate }}</h1>
    <!-- end page-header -->
    <!-- begin row -->
    <div class="row">
        <!-- begin col-6 -->
        <div class="col-md-12">
            <!-- begin panel -->
            <div class="panel panel-inverse" data-sortable-id="form-stuff-1">
                <div class="panel-heading">
                    <h4 class="panel-title">{{ 'REGISTER_KOR' | translate }}</h4>
                </div>
                <div class="panel-body">
                    <form name="formCadFiscalizacao" role="form" ng-submit="verificaInserirAtualizarKor(novokor.IdKor)">
                        <div class="row">
                            <div class="col-md-3">
                                <label class="control-label">{{ 'NAME_KOR' | translate }}:</label>
                                <div class="m-b-15">
                                    <input type="text" class="form-control" ng-model="novokor.Nome" required>
                                </div>
                            </div>
                            <div class="col-md-3">

                                <label class="control-label">{{ 'MODEL' | translate }}:</label>
                                <div class="m-b-15">
                                    <input type="text" class="form-control" ng-model="novokor.Modelo" required>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <label class="">{{ 'SERIAL_NUMBER' | translate }}:</label>
                                <input type="text" required="" ng-model="novokor.NumeroSerie" class="form-control ng-pristine ng-untouched ng-invalid ng-invalid-required">
                            </div>

                            <div class="col-md-3">
                                <label class="">Status:</label>
                                <div class="m-b-5">
                                    <select class="form-control" id="ddlAtivo" ng-model="novokor.Ativo" required>
                                        <option value="">{{ 'SELECT' | translate }}...</option>
                                        <option value="0">{{ 'INACTIVE' | translate }}</option>
                                        <option value="1">{{ 'ACTIVE' | translate }}</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <button type="submit" class="btn btn-sm btn-success" ng-disabled="formCadFiscalizacao.$invalid"><i class="fa fa-play" style="font-size:10px;"></i>&nbsp;&nbsp;{{ 'SUBMIT' | translate }}</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
            <!-- end panel -->

            <div class="panel panel-inverse" data-sortable-id="table-basic-4">
                <div class="panel-heading">
                    <input type="text" value="" id="txtPesquisarEnviados" class="pull-right" style="color:black" ng-model="kor.$" placeholder="{{ SEARCH_SUBJECT | translate }}" />
                    <h4 class="panel-title">{{ 'KOR' | translate }}</h4>
                </div>
                <div class="panel-body">
                    <table class="table table-striped table-bordered">
                        <thead>
                            <tr>
                                <th width="50">ID</th>
                                <th>KOR</th>
                                <th width="150">{{ 'MODEL' | translate }}</th>
                                <th width="150">{{ 'SERIAL_NUMBER' | translate }}</th>
                                <th width="150">Status</th>
                                <th width="76">{{ 'ACTION' | translate }}</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="kor in Kors | filter:kor:restrict | orderBy:'Nome'">
                                <td>{{kor.IdKor}}</td>
                                <td>{{kor.Nome}}</td>
                                <td>{{kor.Modelo}}</td>
                                <td>{{kor.NumeroSerie}}</td>
                                <td translate>{{kor.Ativo == 1 ? 'ACTIVE':'INACTIVE'}}</td>
                                <td>
                                    <a href="javascript:;" ng-click="pegaDadosKor(kor)" class="btn" style="padding: 2px"><i class="fa fa-2x fa-pencil" style="color: #777; font-size: 16px;"></i></a>
                                    <a href="javascript:;" ng-click="excluirKor(kor)" class="btn" style="padding: 2px"><i class="fa fa-2x fa-times" style="color: #777; font-size: 20px;"></i></a>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                    <div class="email-footer clearfix">
                        <custom-pager page="{{page}}" pages-count="{{pagesCount}}" total-count="{{totalCount}}" search-func="carregarKor(page)"></custom-pager>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>
