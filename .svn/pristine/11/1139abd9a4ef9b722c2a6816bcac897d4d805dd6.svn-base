                                                        <label>{{ 'OPERATION_NAME' | translate }}: </label>
                                                        <br />
                                                        <label>{{ 'NATURE' | translate }}: </label>
                                                        <br />
                                                        <label>{{ 'RESPONSIBLE' | translate }}: </label>
                                                        <br />
                                                        <label>{{ 'OFFICE' | translate }}: </label>
                                                        <br />
                                                        <label>{{ 'INSTITUTION' | translate }}: </label>
                                                        <br />
                                                        <label>{{ 'EMAIL' | translate }}: </label>
                                                        <br />
                                                        <label>{{ 'TELEPHONE' | translate }}: </label>
                                                        <br />
                                                        <label>{{ 'START' | translate }}: </label>
                                                        <br />
                                                        <label>{{ 'END' | translate }}: </label>
                                                        <br />
                                                        <label>{{ 'OPERATION_DESCRIPTION' | translate }}: </label>

                                                    </div>
                                                    <div class="col-md-9">
                                                        <label>{{novaOperacao.DescOperacao}}</label>
                                                        <br />
                                                        <label>{{natureza.DescnaturezaOperacao}}</label>
                                                        <br />
                                                        <label>{{novaOperacao.NomeResponsavel}}</label>
                                                        <br />
                                                        <label>{{novaOperacao.CargoResponsavel}}</label>
                                                        <br />
                                                        <label>{{novaOperacao.InstituicaoResponsavel}}</label>
                                                        <br />
                                                        <label>{{novaOperacao.EmailResposnsavel}}</label>
                                                        <br />
                                                        <label>{{novaOperacao.TelResponsavel | filterTelefone}}</label>
                                                        <br />
                                                        <label>{{novaOperacao.DataCriado | filterDataHora}}</label>
                                                        <br />
                                                        <label>{{novaOperacao.DataFinalOperacao | filterDataHora}}</label>
                                                        <br />
                                                        <label>{{novaOperacao.DescObservacao}}</label>

                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="text-center" style="background: #f0f3f4; color: #242a30; border-radius: 5px; margin-bottom: 20px; font-size: 18px; padding: 5px;">{{ 'INSPECTION' | translate }}</div>
