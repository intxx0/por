<ul class="pagination pagination-sm m-t-0 m-b-0 pull-right">
    <li class="disabled"><a ng-hide="page == 0" ng-click="search(0)"><i class="fa fa-angle-double-left"></i></a></li>
    <li class="disabled"><a ng-hide="page == 0" ng-click="search(page-1)"><i class="fa fa-angle-left"></i></a></li>
    <li><a ng-hide="page == pagesCount - 1" ng-click="search(pagePlus(1))"><i class="fa fa-angle-right"></i></a></li>
    <li><a ng-hide="page == pagesCount - 1" ng-click="search(pagesCount - 1)"><i class="fa fa-angle-double-right"></i></a></li>
</ul>
