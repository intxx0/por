<div>
    <!-- begin breadcrumb -->
    <ol class="breadcrumb pull-right">
        <li><a href="javascript:;">{{ 'HOME' | translate }}</a></li>
        <li class="active">{{ 'INSPECTION' | translate }}</li>
    </ol>
    <!-- end breadcrumb -->
    <!-- begin page-header -->
    <h1 class="page-header">{{ 'INSPECTION' | translate }}</h1>
    <!-- end page-header -->
    <!-- begin row -->
    <div class="row">
        <!-- begin col-6 -->
        <div class="col-md-12">
            <!-- begin panel -->
            <div class="panel panel-inverse" data-sortable-id="form-stuff-1">
                <div class="panel-heading">
                    <h4 class="panel-title">{{ 'SEARCH' | translate }}</h4>
                </div>
                <div class="panel-body">
                    <form name="formFiscalizacao" role="form" ng-submit="verificaInserirAtualizarSubTipoFiscalizacao(modelsubtipofiscalizacao.IdSubTipoFiscalizacao)">
                        <div class="row">
                            <div class="col-md-2">
                                <div class="form-group">
                                    <label>{{ 'NATURE' | translate }}:</label>
                                    <select class="form-control" id="ddlNaturezaOperacao" ng-model="modelFiscalizacao.IdNaturezaOperacao" ng-options="option.IdNaturezaOperacao as option.DescnaturezaOperacao for option in NaturezaOperacao"></select>
                                    <input type="hidden" name="modelFiscalizacao.IdNaturezaOperacao" ng-value="modelFiscalizacao.IdNaturezaOperacao" />
                                </div>
                            </div>
                            <div class="col-md-2">
                                <div class="form-group">
                                    <label>KOR:</label>
                                    <select class="form-control" id="ddlKor" ng-model="modelFiscalizacao.IdKor" ng-options="option.IdKor as option.Nome for option in Kor"></select>
                                    <input type="hidden" name="modelFiscalizacao.IdKor" ng-value="modelFiscalizacao.IdKor" />
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>{{ 'OPERATION' | translate }}:</label>
                                    <select class="form-control" id="ddlOperacao" ng-model="modelFiscalizacao.IdOperacao" ng-options="option.IdOperacao as option.DescOperacao for option in Operacao"></select>
                                    <input type="hidden" name="modelFiscalizacao.IdOperacao" ng-value="modelFiscalizacao.IdOperacao" />
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>{{ 'INSTITUTION' | translate }}:</label>
                                    <select class="form-control" id="ddlInstituicao" ng-model="modelFiscalizacao.IdInstituicao" ng-options="option.IdInstituicao as option.NomeInstituicao for option in Instituicao"></select>
                                    <input type="hidden" name="modelFiscalizacao.IdInstituicao" ng-value="modelFiscalizacao.IdInstituicao" />
                                </div>
                            </div>
                            <div class="col-md-2" style="height:71px;">
                                <button type="button" class="btn btn-sm btn-success" style="margin-top:23px; float:right;" ng-click="clearFields()"><i class="fa fa-eraser" style="font-size:10px;"></i>&nbsp;&nbsp;{{ 'CLEAR' | translate }}</button>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-2">
                                <div class="form-group">
                                    <label>{{ 'PERIOD' | translate }}:</label>
                                    <input type="text" ng-model="modelFiscalizacao.DataInicio" placeholder="" id="txtDataInicio" class="form-control" ui-mask="99/99/9999" ui-mask-placeholder ui-mask-placeholder-char="_">
                                </div>
                            </div>
                            <div class="col-md-2">
                                <div class="form-group">
                                    <label>&nbsp;</label>
                                    <input type="text" ng-model="modelFiscalizacao.DataTermino" placeholder="" id="txtDataTermino" class="form-control" ui-mask="99/99/9999" ui-mask-placeholder ui-mask-placeholder-char="_">
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>{{ 'INSPECT_TYPES' | translate }}:</label>
                                    <select class="form-control" id="ddlTipoFiscalizacao" ng-model="modelFiscalizacao.IdTipoFiscalizacao" ng-options="option.IdTipoFiscalizacao as option.DescTipoFiscalizacao for option in TiposFiscalizacao" required></select>
                                    <input type="hidden" name="modelFiscalizacao.IdTipoFiscalizacao" ng-value="modelFiscalizacao.IdTipoFiscalizacao" />
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>{{ 'INSPECTION_SUBTYPE' | translate }}:</label>
                                    <select class="form-control" id="ddlSubTipoFiscalizacao" ng-model="modelFiscalizacao.IdSubTipoFiscalizacao" ng-options="option.IdSubTipoFiscalizacao as option.DescSubTipoFiscalizacao for option in SubTiposFiscalizacao" required></select>
                                    <input type="hidden" name="modelFiscalizacao.IdTipoFiscalizacao" ng-value="modelFiscalizacao.IdTipoFiscalizacao" />
                                </div>
                            </div>
                            <div class="col-md-2">
                                <button type="submit" class="btn btn-sm btn-success" ng-click="doSearch(modelFiscalizacao)" style="margin-top:23px; float:right;"><i class="fa fa-search" style="font-size:10px;"></i>&nbsp;&nbsp;{{ 'SEARCH' | translate }}</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
            <!-- end panel -->

            <div class="panel panel-inverse" data-sortable-id="table-basic-4">
                <div class="panel-heading">
                    <span class="pull-right">Total: {{TotalFiscalizacoes}} {{ 'INSPECTIONS' | translate }}</span>
                    <h4 class="panel-title">{{ 'INSPECTIONS' | translate }}</h4>
                </div>
                <div class="panel-body">
                    <table class="table table-striped table-bordered">
                        <thead>
                            <tr>
                                <th>{{ 'OPERATION' | translate }}</th>
                                <th>{{ 'EMISSION_GUIDE' | translate }}</th>
                                <th>{{ 'EMISSION_NUMBER' | translate }}</th>
                                <th>{{ 'INSPECTION' | translate }}</th>
                                <th>{{ 'INSPECTION_DATE' | translate }}</th>
                                <th width="76">{{ 'ACTION' | translate }}</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="fiscalizacao in Fiscalizacoes">
                                <td>{{fiscalizacao.DescOperacao}}</td>
                                <td>{{fiscalizacao.GuiaEmissor}}</td>
                                <td>{{fiscalizacao.GuiaNumero}}</td>
                                <td>{{fiscalizacao.DescFiscalizacao}}</td>
                                <td>{{fiscalizacao.DataFiscalizacao | date:'dd/MM/yyyy HH:mm:ss'}}</td>
                                <td>
                                    <a href="javascript:;" ng-click="loadFiscalizacao(fiscalizacao.IdFiscalizacao)" class="btn" style="padding:2px;"><i class="fa fa-2x fa-search" style="color: #777; font-size: 16px;"></i></a>
                                    <a href="javascript:;" ng-click="loadFichaDigital(fiscalizacao.IdFiscalizacao)" class="btn" style="padding:2px;" ng-show="fiscalizacao.NumeroAutuacao"><i class="fa fa-2x fa-file-text-o" style="color: #777; font-size: 16px;"></i></a>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                    <!--<div class="email-footer clearfix">
                        <custom-pager page="{{page}}" pages-count="{{pagesCount}}" total-count="{{totalCount}}" search-func="carregarSubTipoFiscalizacao(page)"></custom-pager>
                    </div>-->
                </div>
            </div>

        </div>
    </div>
</div>
