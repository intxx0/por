<div>
    <!-- begin breadcrumb -->
    <ol class="breadcrumb pull-right">
        <li><a href="javascript:;">{{ 'HOME' | translate }}</a></li>
        <li class="active">{{ 'REGISTER_INSPECTION_SUBTYPE' | translate }}</li>
    </ol>
    <!-- end breadcrumb -->
    <!-- begin page-header -->
    <h1 class="page-header">{{ 'REGISTER_INSPECTION_SUBTYPE' | translate }}</h1>
    <!-- end page-header -->
    <!-- begin row -->
    <div class="row">
        <!-- begin col-6 -->
        <div class="col-md-12">
            <!-- begin panel -->
            <div class="panel panel-inverse" data-sortable-id="form-stuff-1">
                <div class="panel-heading">
                    <h4 class="panel-title">{{ 'REGISTER_INSPECTION_SUBTYPE' | translate }}</h4>
                </div>
                <div class="panel-body">
                    <form name="formCadSubTipoFiscalizacao" role="form" ng-submit="verificaInserirAtualizarSubTipoFiscalizacao(modelsubtipofiscalizacao.IdSubTipoFiscalizacao)">
                        <div class="row">
                            <div class="col-md-6">
                                <label class="control-label">{{ 'NAME_INSPECTION_SUBTYPE' | translate }}:</label>
                                <div class="m-b-15">
                                    <input type="text" class="form-control" ng-model="modelsubtipofiscalizacao.DescSubTipoFiscalizacao" required>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>{{ 'INSPECT_TYPES' | translate }}:</label>
                                    <select class="form-control" id="ddlTipFiscalizacao" ng-model="modelsubtipofiscalizacao.IdTipoFiscalizacao" ng-options="option.IdTipoFiscalizacao as option.DescTipoFiscalizacao for option in TiposFiscalizacao" required></select>
                                    <input type="hidden" name="modelsubtipofiscalizacao.IdTipoFiscalizacao" ng-value="modelsubtipofiscalizacao.IdTipoFiscalizacao" />
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <button type="submit" class="btn btn-sm btn-success" ng-disabled="formCadSubTipoFiscalizacao.$invalid"><i class="fa fa-play" style="font-size:10px;"></i>&nbsp;&nbsp;{{ 'SUBMIT' | translate }}</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
            <!-- end panel -->

            <div class="panel panel-inverse" data-sortable-id="table-basic-4">
                <div class="panel-heading">
                    <input type="text" value="" id="txtPesquisarEnviados" class="pull-right" style="color:black" ng-model="subtipofiscalizacao.$" placeholder="{{ SEARCH_SUBJECT | translate }}" />
                    <h4 class="panel-title">{{ 'INSPECTION_SUBTYPE' | translate }}</h4>
                </div>
                <div class="panel-body">
                    <table class="table table-striped table-bordered">
                        <thead>
                            <tr>
                                <th width="50">ID</th>
                                <th>{{ 'INSPECTION_SUBTYPE' | translate }}</th>
                                <th>{{ 'INSPECTION_TYPE' | translate }}</th>
                                <th width="76">{{ 'ACTION' | translate }}</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="subtipofiscalizacao in Missoes | filter:subtipofiscalizacao:restrict | orderBy:'Nome'">
                                <td>{{subtipofiscalizacao.IdSubTipoFiscalizacao}}</td>
                                <td>{{subtipofiscalizacao.DescSubTipoFiscalizacao}}</td>
                                <td>{{subtipofiscalizacao.DescTipoFiscalizacao}}</td>
                                <td>
                                    <a href="javascript:;" ng-click="pegaDadosSubTipoFiscalizacao(subtipofiscalizacao)" class="btn" style="padding: 2px"><i class="fa fa-2x fa-pencil" style="color: #777; font-size: 16px;"></i></a>
                                    <a href="javascript:;" ng-click="excluirSubTipoFiscalizacao(subtipofiscalizacao)" class="btn" style="padding: 2px"><i class="fa fa-2x fa-times" style="color: #777; font-size: 20px;"></i></a>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                    <div class="email-footer clearfix">
                        <custom-pager page="{{page}}" pages-count="{{pagesCount}}" total-count="{{totalCount}}" search-func="carregarSubTipoFiscalizacao(page)"></custom-pager>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>
