                                                                <label>{{ 'NAME' | translate }}: </label>{{kor.Nome}}
                                                                <br />
                                                                <label>{{ 'MODEL' | translate }}: </label>{{kor.Modelo}}
                                                                <br />
                                                                <label>{{ 'SERIAL_NUMBER' | translate }}: </label>{{kor.NumeroSerie}}
                                                                <br />
                                                            </label>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="text-center" style="background: #f0f3f4; color: #242a30; border-radius: 5px; margin-bottom: 20px; font-size: 18px; padding: 5px;">{{ 'USERS' | translate }}</div>
