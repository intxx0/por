                                                                <label>{{ 'EMAIL' | translate }}: </label> <label style="margin-left: 2px;">{{usuariosOperacao.DescEmailUsuario}}</label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </fieldset>
                                            <div class="row">
