                                                    <button type="button" class="btn btn-sm btn-success" ng-click="habilitaDesabilitaAbaCadUsuarios()" style="float:left;"><i class="fa fa-arrow-left" style="font-size:10px;"></i>&nbsp;&nbsp;{{ 'BACK' | translate }}</button>
                                                    <button type="button" class="btn btn-sm btn-success" ng-click="verificaIdFiscalizacaoOnLine(novaOperacao,natureza)" style="margin-left:10px; float:right;"><i class="fa fa-check" style="font-size:10px;"></i>&nbsp;&nbsp;{{ 'CONFIRM' | translate }}</button>
                                                    <button type="button" class="btn btn-sm btn-success" ng-click="cancelarOperacao()" style="float:right;"><i class="fa fa-times"></i>&nbsp;&nbsp;{{ 'CANCEL' | translate }}</button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div id="tabsucesso" ng-class="tabsOperacao.tabOperacaoFinalizada.contentAtivar" ng-show="tabsOperacao.tabOperacaoFinalizada.contentHabilitar">
                                    <div id="step4" role="tabpanel" class="" aria-hidden="true">
                                        <div class="widget widget-stats bg-green">
                                            <div class="stats-icon">
                                                <i class="fa fa-thumbs-up"></i>
                                            </div>
                                            <div class="stats-info text-center">
                                                <h3>{{ 'SUCCESS_REGISTRATION' | translate }}!</h3>
                                                <p class="ng-binding">{{Operacao}}</p>
                                                <p class="ng-binding" style="font-size: 40px;">
                                                    <i class="fa fa-check-square-o"></i>
                                                </p>
                                            </div>
                                            <div class="stats-link">
                                                <h3>
                                                    <a href="javascript:;" ng-click="habilitaDesabilitaAbaPesquisa()">
                                                        {{ 'OPERATIONS_LIST' | translate }}Lista de operações
                                                        <i class="fa fa-arrow-circle-o-right"></i>
                                                    </a>
                                                </h3>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                </div>
                        </div>
                    </form>
                </div>
            </div>
            <!-- end panel -->
        </div>
        <!-- end col-12 -->
    </div>
    <!-- end row -->
</div>

